{
  "name": "tinper-neoui",
  "version": "3.2.5",
  "description": "UI Framework Used For Enterprise.",
  "main": "src/index",
  "scripts": {
    "clean": "gulp clean",
    "r-watch": "rollup -c -w",
    "r-build": "rollup -c ",
    "build": "webpack --colors --progress --env.mode=build",
    "dev": "webpack --progress --colors --env.mode=dev",
    "plugin": "npm run neoui-build && gulp buildcssplugin && node concat",
    "es": "babel src -d lib",
    "product": "npm run es &&  npm run r-build && gulp dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 ",
    "docs": "node bin/mdconcat.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iuap-design/tinper-neoui.git"
  },
  "keywords": [
    "neoui",
    "iUAP",
    "Design",
    "UI",
    "Framework"
  ],
  "author": "yonyou FED",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iuap-design/tinper-neoui/issues"
  },
  "homepage": "https://github.com/iuap-design/tinper-neoui#readme",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "browser-sync": "^2.12.8",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "deepmerge": "^1.3.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.5.0",
    "file": "^0.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-base64": "^0.1.3",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
