    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "mocha": "^3.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-multidest": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "webpack": "^2.2.1",
